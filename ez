--[[
	Blacklist Source Code
]]
local Blacklist = {
    "ROBLOX", -- <--- Blacklisted Users
    "error45421"
}

for _, Player in pairs(Blacklist) do
    if game.Players.LocalPlayer.Name == Player then
        game.Players.LocalPlayer:Kick("YOU HAVE BEEN BLACKLISTED! YOU CANNOT APPEAL IT")
else
print("dd")
end
end
